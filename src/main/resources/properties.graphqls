    xidStart: Boolean
    patternSyntax: Boolean
    patternWhiteSpace: Boolean

    # CJK properties
    ideographic: Boolean
    unifiedIdeograph: Boolean
    radical: Boolean
    idsBinaryOperator: Boolean
    idsTrinaryOperator: Boolean
    unicodeRadicalStroke: [String]

    # Misc properties
    math: Boolean
    quotationMark: Boolean
    dash: Boolean
    hyphen: Boolean
    sentenceTerminal: Boolean
    terminalPunctuation: Boolean
    diacritic: Boolean
    extender: Boolean
    graphemeBase: Boolean
    graphemeExtend: Boolean
    graphemeLink: Boolean
    unicode1Name: String
    isoComment: String
    indicPositionalCategory: String
    indicSyllabicCategory: String

    # Contributory properties
    otherAlphabetic: Boolean
    otherDefaultIgnorableCodePoint: Boolean
    otherGraphemeExtend: Boolean
    otherIdStart: Boolean
    otherIdContinue: Boolean
    otherLowercase: Boolean
    otherMath: Boolean
    otherUppercase: Boolean
    jamoShortName: String
