type Properties {
    # general properties
    name: String
    nameAlias: [NameAlias]
    block: String
    age: String
    generalCategory: String
    script: String
    scriptExtensions: [String]
    whiteSpace: Boolean
    alphabetic: Boolean
    hangulSyllabeType: String
    nonCharacterCodePoint: Boolean
    defaultIgnorableCodePoint: Boolean
    deprecated: Boolean
    logicalOrderException: Boolean
    variationSelector: Boolean

    # case properties
    uppercase: Boolean
    lowercase: Boolean
    lowercaseMapping: [String]
    titlecaseMapping: [String]
    uppercaseMapping: [String]
    caseFolding: [String]
    simpleLowercaseMapping: String
    simpleTitlecaseMapping: String
    simpleUppercaseMapping: String
    simpleCaseFolding: String
    softDotted: Boolean
    cased: Boolean
    caseIgnorable: Boolean
    changesWhenLowercased: Boolean
    changesWhenUppercased: Boolean
    changesWhenTitlecased: Boolean
    changesWhenCasefolded: Boolean
    changesWhenCasemapped: Boolean

    # numeric properties
    numericValue: String
    numericType: String
    hexDigit: Boolean
    asciiHexDigit: Boolean

    # normalization properties
    canonicalCombiningClass: Int
    decompositionMapping: [String]
    compositionExclusion: Boolean
    fullCompositionExclusion: Boolean
    decompositionType: String
    fcNfkcClosure: [String]
    nfcQuickCkeck: String
    nfkcQuickCheck: String
    nfdQuickCheck: String
    nfkdQuickCheck: String
    expandsOnNfc: Boolean
    expandsOnNfkc: Boolean
    expandsOnNfd: Boolean
    expandsOnNfkd: Boolean
